{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: clang++ build active file",
            "command": "/usr/bin/clang++",
            "args": [
                "-fcolor-diagnostics",
                "-fansi-escape-codes",
                "-g",
                "${workspaceFolder}/*.cpp",
                "-o",
                "${workspaceFolder}/myProgram.out",
                "-I/opt/homebrew/opt/lapack/include",
                "-I/Users/shiqing/anaconda3/envs/py311/include/eigen3",
                "-I/Users/shiqing/anaconda3/envs/py311/include",
                "-I/Users/shiqing/anaconda3/envs/py311/include/xtensor-blas",
                "-I/Users/shiqing/anaconda3/envs/py311/include/xtensor-python",
                "-I/Users/shiqing/anaconda3/envs/py311/lib/python3.11/site-packages/numpy/core/include",
                "--std=c++14",
                "-L/opt/homebrew/opt/lapack/lib",
                "-llapack",
                "-lblas"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}